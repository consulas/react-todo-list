{
  "name": "react-todo-list",
  "version": "1.0.0",
  "description": "A simple todo app with Express and React",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "webpack serve --mode development",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "webpack --mode production"
  },
  "keywords": [
    "express",
    "react",
    "webpack",
    "nodemon",
    "todo"
  ],
  "author": "Kurt Weiberth",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}